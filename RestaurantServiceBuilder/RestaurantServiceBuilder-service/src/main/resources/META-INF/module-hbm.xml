<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="org.liferayasif.sb.model.Menu" />
	<class name="org.liferayasif.sb.model.impl.MenuImpl" table="AA_Menu">
		<id name="menuId" type="long">
			<generator class="assigned" />
		</id>
		<property column="type_" name="type" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="foodName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="price" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>