<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="org.liferayasif.sb">
	<namespace>AA</namespace>
	
	<entity local-service="true" name="Menu" remote-service="false">

		<column name="menuId" primary="true" type="long" />
		<column name="type" type="String" />
		<column name="foodName" type="String" />
		<column name="price" type="long" />
		
		<finder name="Type" return-type="Collection">
			<finder-column name="type" />
		</finder>

	</entity>
</service-builder>